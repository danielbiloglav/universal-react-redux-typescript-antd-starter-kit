    "@types/react-dom": "^0.14.23",
    "@types/react-helmet": "0.0.28",
    "@types/react-redux": "^4.4.36",
    "@types/react-router": "^3.0.6",
    "@types/redux": "^3.6.0",
    "@types/webpack": "^2.2.7",
    "@types/webpack-dev-middleware": "^1.9.1",
    "@types/webpack-hot-middleware": "^2.15.0",
    "cross-env": "^3.1.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "tslint": "^4.5.1",
