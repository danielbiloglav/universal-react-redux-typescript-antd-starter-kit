{
  "name": "universal-react-redux-typescript-antd-starter-kit",
  "version": "0.0.1",
  "description": "A minimal starter kit with React, Redux, TypeScript, server side rendering, hot reloading, Webpack 2 and Ant Design",
  "scripts": {
    "start": "npm run start:dev",
    "start:prod": "cross-env NODE_ENV=production node dist/server/src/server.js",
    "start:dev": "ts-node ./src/server.tsx",
    "debug": "ts-node --inspect=9229 ./src/server.tsx",
    "build:dev": "cross-env NODE_ENV=development ts-node scripts/build.ts",
    "build:prod": "cross-env NODE_ENV=production ts-node scripts/build.ts",
    "lint": "tslint -c tslint.json ./src/**/*.ts*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danielbiloglav/universal-react-redux-typescript-antd-starter-kit.git"
  },
  "author": "Fran√ßois Nguyen (https://github.com/lith-light-g), Daniel Biloglav (https://github.com/danielbiloglav)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielbiloglav/universal-react-redux-typescript-antd-starter-kit/issues"
  },
  "homepage": "https://github.com/danielbiloglav/universal-react-redux-typescript-antd-starter-kit#readme",
  "dependencies": {
    "antd": "^2.8.0",
    "autoprefixer": "^6.7.6",
    "awesome-typescript-loader": "^3.0.8",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-import": "^1.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.2",
    "express": "^4.15.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "isomorphic-fetch": "^2.2.1",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "postcss-loader": "^1.3.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "serialize-javascript": "^1.3.0",
    "style-loader": "^0.13.2",
    "ts-node": "^2.1.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1"
